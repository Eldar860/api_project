{
  "name": "car-rental-api",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "ts-node src/seed.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d src/ormconfig.ts",
    "migrate:generate": "npm run typeorm migration:generate -d ./ormconfig.ts",
    "migrate:run": "npm run typeorm migration:run -d ./ormconfig.ts",
    "migrate:revert": "npm run typeorm migration:revert -d ./ormconfig.ts"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "dotenv": "^16.6.0",
    "express": "^5.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "main": "index.js",
  "repository": {
